@model IEnumerable<CarInsurance.Models.Insuree>
@{
    ViewData["Title"] = "Admin View";
}
<h2>Admin View</h2>
<table class="table">
    <thead>
        <tr><th>First</th><th>Last</th><th>Email</th><th>Quote</th></tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.EmailAddress</td>
                <td>@item.Quote.ToString("C")</td>
            </tr>
        }
    </tbody>
</table>