@model CarInsurance.Models.Insuree
@{
    ViewData["Title"] = "Create Insuree";
}
<h2>Create Insuree</h2>
<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <!-- loop through fields except Quote -->
    <div class="form-group">
        <label asp-for="FirstName"></label>
        <input asp-for="FirstName" class="form-control" />
        <span asp-validation-for="FirstName" class="text-danger"></span>
    </div>
    <!-- Repeat for LastName, EmailAddress, DateOfBirth, CarYear, CarMake, CarModel -->
    <div class="form-group">
        <label asp-for="DUI">DUI</label>
        <input asp-for="DUI" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="SpeedingTickets"></label>
        <input asp-for="SpeedingTickets" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="CoverageType">Coverage Type (Full = true)</label>
        <input asp-for="CoverageType" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
